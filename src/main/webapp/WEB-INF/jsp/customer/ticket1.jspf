<meta name="viewport" content="width=device-width, initial-scale=1">
<link rel="stylesheet" href="https://use.fontawesome.com/releases/v5.7.0/css/all.css" integrity="sha384-lZN37f5QGtY3VHgisS14W3ExzMWZxybE1SJSEsQp9S+oqd12jhcu+A56Ebc1zFSJ" crossorigin="anonymous">
<div class="Polaris-Tabs__Panel" role="tabpanel" aria-labelledby="help" tabindex="-1" id="support_tabc">
	<div class="Polaris-Card__Section">
    <div class="Polaris-Page__Content">
      	<div style="-top-bar-background: #00848e; - -top-bar-color: #f9fafb; - -top-bar-background-lighter: #1d9ba4;">
		</div>
		<br/> 
            <div style="--top-bar-background:#00848e; --top-bar-color:#f9fafb; --top-bar-background-lighter:#1d9ba4;">
  <div class="Polaris-Card">
    <div class="Polaris-ResourceList__ResourceListWrapper">
      <ul class="Polaris-ResourceList" aria-live="polite">
      <li class="Polaris-ResourceList__ItemWrapper">
      <c:forEach items="${listSupportData}" var="val">
      <c:if  test="${val.status == 'OPEN'}"> 
      <%@ include file="reply.jspf" %>
      	</c:if> 
       <c:if  test="${val.status == 'CLOSE'}"> 
            <h3 class="h4 mar-no">Issue Closed...</h3>
         </c:if> 
      </li>
        <li class="Polaris-ResourceList__ItemWrapper">
          <div class="Polaris-ResourceList-Item" data-href="customers/341"><a aria-describedby="341" aria-label="View details for Mae Jemison" class="Polaris-ResourceList-Item__Link" tabindex="0" id="ResourceListItemOverlay21" href="customers/341" data-polaris-unstyled="true"></a>
            <div class="Polaris-ResourceList-Item__Container" id="341">
              <div class="Polaris-ResourceList-Item__Owned">
                <div class="Polaris-ResourceList-Item__Media"><span aria-label="Mae Jemison" role="img" class="Polaris-Avatar Polaris-Avatar--styleSix Polaris-Avatar--sizeMedium Polaris-Avatar--hasImage"><img src="data:image/svg+xml;base64," class="Polaris-Avatar__Image" alt="" role="presentation"></span></div>
              </div>
              <div class="Polaris-ResourceList-Item__Content">
                <h3><span class="Polaris-TextStyle--variationStrong">${val.subject}</span></h3>
                <div>${val.description} <br><br>
                <i class="far fa-clock"></i>
                ${val.createdAt}
                </div>
              </div>
            </div>
          </div>
        </li>
        </c:forEach>
      </ul>
    </div>
  </div>
</div>
  
          </div>
        </div>
      </div>